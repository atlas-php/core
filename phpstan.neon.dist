includes:
    - vendor/nunomaduro/larastan/extension.neon

parameters:
    paths:
        - src
        - tests
    level: 7
    tmpDir: build/phpstan
    checkMissingIterableValueType: true
    inferPrivatePropertyTypeFromConstructor: true
    treatPhpDocTypesAsCertain: true
    parallel:
        maximumNumberOfProcesses: 0
